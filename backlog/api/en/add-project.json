{
  "name": "AddProject",
  "summary": "Add Project",
  "description": "Adds new project.",
  "method": "POST",
  "url": "/api/v2/projects",
  "scope": null,
  "formParams": [
    {
      "name": "name",
      "optional": false,
      "array": false,
      "type": "String",
      "description": "Project Name"
    },
    {
      "name": "key",
      "optional": false,
      "array": false,
      "type": "String",
      "description": "Project Key (Uppercase letters (A-Z), numbers (0-9) and underscore (_) can be used.)"
    },
    {
      "name": "chartEnabled",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Enable chart"
    },
    {
      "name": "useResolvedForChart",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Consider Resolved and statuses after as Closed"
    },
    {
      "name": "subtaskingEnabled",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Enable subtasking"
    },
    {
      "name": "projectLeaderCanEditProjectLeader",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Allow project administrators to manage each other"
    },
    {
      "name": "useWiki",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Enable Wiki"
    },
    {
      "name": "useFileSharing",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Enable shared files"
    },
    {
      "name": "useWikiTreeView",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Enable Wiki tree view"
    },
    {
      "name": "useSubversion",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Enable Subversion"
    },
    {
      "name": "useGit",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Enable Git"
    },
    {
      "name": "useOriginalImageSizeAtWiki",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Display images in Wikis in their original size"
    },
    {
      "name": "textFormattingRule",
      "optional": true,
      "array": false,
      "type": "String",
      "description": "Formatting rules \u201cbacklog\u201d or \u201cmarkdown\u201d"
    },
    {
      "name": "useDevAttributes",
      "optional": true,
      "array": false,
      "type": "Boolean",
      "description": "Enable priorities, versions and milestones"
    }
  ],
  "roles": [
    "Administrator"
  ],
  "apiDocumentUrl": "https://developer.nulab.com/docs/backlog/api/2/add-project"
}